{
    "framework": "vite",
    "buildCommand": "echo 'Using pre-built files'",
    "outputDirectory": ".",
    "rewrites": [
      {
        "source": "/admin/(.*)",
        "destination": "/api/proxy/admin/$1"
      },
      {
        "source": "/auth/(.*)",
        "destination": "/api/proxy/auth/$1"
      },
      {
        "source": "/((?!assets|api).*)",
        "destination": "/index.html"
      }
    ]
  }
  